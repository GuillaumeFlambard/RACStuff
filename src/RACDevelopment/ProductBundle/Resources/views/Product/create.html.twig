{% extends 'RACDevelopmentCommonBundle::layout.html.twig' %}

{% block title %}
    <h2>{{ 'rac_develoment.product.title'|trans() }}</h2>&nbsp;<h3>{{ 'rac_develoment.common.general.create_title'|trans() }}</h3>
{% endblock %}
{% block content %}
<form action="{{ path(route) }}" method="post" {{ form_enctype(form) }} novalidate="novalidate">
    {% for child in form.children %}
    {{ form_row(child) }}
    {% endfor %}
    <br>
    <input class="btn btn-success" type="submit" value="{{ 'rac_develoment.common.general.submit'|trans() }}"/>
</form>
{% endblock %}
